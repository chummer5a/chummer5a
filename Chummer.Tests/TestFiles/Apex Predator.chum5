    <quality>
      <guid>a8499aed-3928-437f-a128-4a5835fc1db4</guid>
      <name>Gremlins</name>
      <extra />
      <bp>-4</bp>
      <implemented>True</implemented>
      <contributetolimit>True</contributetolimit>
      <doublecareer>True</doublecareer>
      <canbuywithspellpoints>False</canbuywithspellpoints>
