<chummer>
  <limbcounts>
    <limb>
      <name>4 (2 arms, 2 legs)</name>
      <limbcount>4</limbcount>
      <exclude>torso,skull</exclude>
    </limb>
    <limb>
      <name>5 (2 arms, 2 legs, skull)</name>
      <limbcount>5</limbcount>
      <exclude>torso</exclude>
    </limb>
    <limb>
      <name>5 (2 arms, 2 legs, torso)</name>
      <limbcount>5</limbcount>
      <exclude>skull</exclude>
    </limb>
    <limb>
      <name>6 (2 arms, 2 legs, torso, skull)</name>
      <limbcount>6</limbcount>
      <exclude />
    </limb>
  </limbcounts>
  <pdfarguments>
    <pdfargument>
      <name>Web Browser</name>
      <value>"file://{absolutepath}#page={page}"</value>
    </pdfargument>
    <pdfargument>
      <name>Acrobat-style</name>
      <value>/A "page={page}" "{localpath}"</value>
    </pdfargument>
    <pdfargument>
      <name>Unix-style</name>
      <value>-p {page} "{localpath}"</value>
    </pdfargument>
    <pdfargument>
      <name>Sumatra - Re-use instance</name>
      <value>-reuse-instance -page {page} "{localpath}"</value>
    </pdfargument>
    <pdfargument>
      <name>Sumatra</name>
      <value>-page {page} "{localpath}"</value>
    </pdfargument>
  </pdfarguments>
  <blackmarketpipelinecategories>
    <category>Armor</category>
    <category>Bioware</category>
    <category>Cyberware</category>
    <category>Drugs</category>
    <category>Electronics</category>
    <category>Vehicles</category>
    <category>Weapons</category>
  </blackmarketpipelinecategories>
</chummer>
