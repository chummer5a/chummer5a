      <extra />
      <pointsperlevel>.5</pointsperlevel>
      <adeptway>.25</adeptway>
      <action />
      <rating>1</rating>
      <extrapointcost>0</extrapointcost>
      <levels>False</levels>
      <maxlevel>0</maxlevel>
      <discounted>False</discounted>
      <discountedgeas>False</discountedgeas>
